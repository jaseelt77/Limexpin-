cat > .npmrc << EOF
registry=https://registry.npmjs.org/
strict-ssl=false
legacy-peer-deps=true
audit=false
loglevel=warn
fetch-timeout=60000
fetch-retry-mintimeout=20000
fetch-retry-maxtimeout=120000
fetch-retries=5
engine-strict=false
save-exact=false
save-prefix=^
optional=false
prefer-offline=false
EOF